config { 
  type: 'table',
  tags: ["demo"],
  schema: "dataform"
}

js {
  var currentDateTime = new Date().toISOString();
}

select
  src.day
  , src.industry_item
  , SUM(src.call_cnt) as calls
  , "${currentDateTime}" as query_datetime
from
  ${ ref('call_delivery') } as src
group by 1, 2
order by 1 asc, 3 desc
