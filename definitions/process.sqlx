config { 
  type: 'table',
  tags: ["demo"],
  schema: "dataform",
  name: "chicken_calls_per_day"
}

js {
  var currentDateTime = new Date().toISOString();
}

select
  day
  , industry_item
  , SUM(call_cnt) as calls
  , "${currentDateTime}" as process_datetime
  
from
  ${ ref('call_delivery') }
where
  industry_item = '치킨'
group by 1, 2
order by 1 asc, 3 desc
