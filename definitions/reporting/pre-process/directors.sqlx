config { 
  type: 'view',
  tags: ["daily"],
  schema: "dataform_imdb_reporting"
}

with director_split as (
select 
  tconst as movie_id,
  split(directors, ",") as director_array
from
  ${ ref('title_crew') }
)

select 
  movie_id,
  director_id
from
  director_split, 
  director_split.director_array as director_id